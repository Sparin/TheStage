<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:TheStage.Elements.Primitives">
    <Style x:Key="CircleStyle">
        <Setter Property="Path.Stroke">
            <Setter.Value>
                <SolidColorBrush Color="DarkRed"/>
            </Setter.Value>
        </Setter>
        <Setter Property="Path.StrokeThickness" Value="3"/>
    </Style>
    <Style x:Key="SquareStyle">
        <Setter Property="Path.Stroke">
            <Setter.Value>
                <SolidColorBrush Color="DeepPink"/>
            </Setter.Value>
        </Setter>
        <Setter Property="Path.StrokeThickness" Value="3"/>
    </Style>
    <Style x:Key="TriangleStyle">
        <Setter Property="Path.Stroke">
            <Setter.Value>
                <SolidColorBrush Color="DarkGreen"/>
            </Setter.Value>
        </Setter>
        <Setter Property="Path.StrokeThickness" Value="3"/>
    </Style>
    <Style x:Key="CrossStyle">
        <Setter Property="Path.Stroke">
            <Setter.Value>
                <SolidColorBrush Color="DarkCyan"/>
            </Setter.Value>
        </Setter>
        <Setter Property="Path.StrokeThickness" Value="3"/>
    </Style>
    
</ResourceDictionary>